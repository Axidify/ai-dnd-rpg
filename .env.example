# Copy this file to .env and add your API key

# Google Gemini API Configuration
GOOGLE_API_KEY=your-google-api-key-here

# Model Selection (optional - defaults to gemini-2.5-pro)
# Options: gemini-2.5-pro, gemini-2.5-flash, gemini-1.5-pro, gemini-1.5-flash
GEMINI_MODEL=gemini-2.5-pro

# Save Directory (optional - defaults to ./saves/)
# SAVES_DIR=./saves
