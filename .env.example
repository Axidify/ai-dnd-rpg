# Copy this file to .env and add your API key

# Google Gemini API Configuration
GOOGLE_API_KEY=your-google-api-key-here

# Model Selection (optional - defaults to gemini-2.5-pro)
# Options: gemini-2.5-pro, gemini-2.5-flash, gemini-1.5-pro, gemini-1.5-flash
GEMINI_MODEL=gemini-2.5-pro

# Save Directory (optional - defaults to ./saves/)
# SAVES_DIR=./saves
# Debug Mode (optional - defaults to false)
# When true, enables verbose logging and file logging automatically
DEBUG=false

# Logging Configuration (optional)
# VERBOSE_LOGGING defaults to the value of DEBUG
# LOG_TO_FILE defaults to the value of DEBUG
# Set explicitly to override:
# VERBOSE_LOGGING=true
# LOG_TO_FILE=true